{
  "expo": {
    "name": "BizManager",
    "slug": "bizmanager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_app.png",
    "scheme": "bizmanager",
    "owner": "caetano.apollo",
    "userInterfaceStyle": "automatic",
    "projectId": "a486bbc9-3668-4092-beca-cc79d8b09b90",
    "fonts": [
      {
        "name": "BebasNeue",
        "asset": "./assets/fonts/BebasNeue-Regular.ttf"
      }
    ],
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.caetanoapollo.bizmanager",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_app.png",
        "backgroundColor": "#2A4D69"
      },
      "package": "com.caetanoapollo.bizmanager",
      "softwareKeyboardLayoutMode": "resize"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon_app.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon_app.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2A4D69"
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a486bbc9-3668-4092-beca-cc79d8b09b90"
      }
    }
  }
}
