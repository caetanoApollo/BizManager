{
  "expo": {
    "name": "BizManager",
    "slug": "BizManager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_app.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "fonts": [
      {
        "name": "BebasNeue",
        "asset": "./assets/fonts/BebasNeue-Regular.ttf"
      }
    ],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon_app.png",
        "backgroundColor": "#2A4D69"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon_app.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon_app.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2A4D69"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}